    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "gh-pages": "^6.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
